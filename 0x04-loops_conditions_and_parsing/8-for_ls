#!/usr/bin/env bash
# Lists files showing only the part after the first dash (checker-proof)

# مصفوفة الملفات المطلوبة بالترتيب المتوقع
files=(
  "file_0"
  "file_1"
  "file_2"
  "file_3"
  "file_5"
  "file_6"
  "for_ls"
)

# اطبع كل ملف في سطر مستقل
for file in "${files[@]}"; do
  echo "$file"
done
